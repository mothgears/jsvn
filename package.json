{
  "name": "jsvn",
  "version": "0.3.1",
  "description": "JavaScript Views Notation",
  "author": "Artem Varshavin",
  "license": "MIT",
  "keywords": [
    "view",
    "notation",
    "gui"
  ],
  "main": "lib/index.js",
  "scripts": {
    "transpile": "babel ./src --out-dir ./lib",
    "build": "parcel build src/index.js --target node --no-minify --out-dir ./ --out-file jsvn.lib.js",
    "esbuild": "esbuild src/script.js --outfile=jsvn.min.mjs --target=edge86 --bundle --minify --sourcemap && esbuild tests/src/index.js --outfile=tests/demo.min.mjs --target=edge86 --bundle --minify --sourcemap",
    "demo": "babel tests/src --out-dir tests/lib && esbuild tests/lib/index.js --outfile=tests/demo.min.js --target=es5 --bundle --minify --sourcemap"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/mothgears/jsvn.git"
  },
  "devDependencies": {
    "@babel/cli": "^7.12.1",
    "@babel/core": "^7.12.3",
    "@babel/plugin-proposal-class-properties": "^7.12.1",
    "@babel/plugin-proposal-private-methods": "^7.12.1",
    "@babel/preset-env": "^7.12.1",
    "esbuild": "^0.7.19"
  },
  "dependencies": {
    "core-js": "^3.6.5",
    "jsman": "^1.1.1"
  }
}
