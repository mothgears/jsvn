{
  "presets": [
    ["@babel/env", {
      "targets": {
        "ie": "11",
        "chrome": "60",
        "firefox": "50"
      },
      "useBuiltIns": "usage",
      "corejs": { "version": 3, "proposals": true }
    }]
  ],
  "plugins": [
    ["@babel/proposal-class-properties" , { "loose": false }],
    "@babel/plugin-proposal-private-methods",
    "@babel/plugin-transform-runtime",
    "module:babel-plugin-add-module-exports"
  ]
}